{
  "expo": {
    "name": "One Enigma",
    "slug": "oneenigma",
    "scheme": "oneenigma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vertical-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/vertical-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#161622"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oneengima.chai"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ]
  }
}
